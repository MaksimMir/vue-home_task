A,CAAA,CACA,qBADA,CAAA,CADA;AAIA,IAAA,MAJA,oBAIA;AACA,UAAA,GAAA,GAAA,QAAA,CAAA,aAAA,CAAA,oBAAA,CAAA;AACA,MAAA,GAAA,CAAA,QAAA,GAAA,IAAA;AACA,UAAA,QAAA,GAAA,IAAA,CAAA,QAAA;AAAA,UAAA,KAAA,GAAA,IAAA,CAAA,KAAA;AACA,UAAA,IAAA,GAAA;AACA,QAAA,IAAA,EAAA,KAAA,IAAA,IAAA,KAAA,cADA;AAEA,QAAA,QAAA,EAAA,QAFA;AAGA,QAAA,KAAA,EAAA;AAHA,OAAA;;AAMA,UAAA,IAAA,CAAA,QAAA,IAAA,IAAA,CAAA,KAAA,EAAA;AACA,QAAA,GAAA,CAAA,QAAA,GAAA,KAAA;AACA,aAAA,MAAA,CAAA,MAAA,CAAA,uBAAA,EAAA,IAAA;AACA;;AAGA,WAAA,KAAA,GAAA,EAAA;AACA,WAAA,IAAA,GAAA,EAAA;AACA;AAtBA,IAlCA;AA0DA,EAAA,OA1DA,qBA0DA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,qBACA,KAAA,CAAA,mBAAA,EADA;;AAAA;AAGA,kCAAA,KAAA,CAAA,MAAA,yCAAA,aAAA,MAAA,CAAA,IAAA,EAAA;AACA,gBAAA,KAAA,CAAA,QAAA,GAAA,KAAA,CAAA,MAAA,CAAA,MAAA,CAAA,IAAA;AACA,gBAAA,KAAA,CAAA,KAAA,GAAA,KAAA,CAAA,MAAA,CAAA,KAAA,CAAA,KAAA;AACA;;AANA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAOA;AAjEA,CAAA","sourcesContent":["<template>\r\n  <div class=\"modal\">\r\n    <div class=\"modal-form\">\r\n      <form>\r\n        <input type=\"text\" v-model=\"date\" placeholder=\"Payment Date\">\r\n        <select v-model=\"category\">\r\n          <option v-for=\"opt in options\" :key=\"opt\" :value=\"opt\">\r\n            {{ opt }}\r\n          </option>\r\n        </select>\r\n        <input type=\"number\" v-model.number=\"value\" placeholder=\"Payment Amount\">\r\n        <input @click=\"onSave\" type=\"button\" value=\"Add  +\">\r\n      </form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapActions } from 'vuex';\r\n\r\n\r\nexport default {\r\n  name: 'ButtonModal',\r\n  data() {\r\n    return {\r\n      date: '',\r\n      category: '',\r\n      value: '',\r\n    }\r\n  },\r\n  computed: {\r\n    getCurrentDate() {\r\n      const today = new Date();\r\n      let day, month, year;\r\n      \r\n      if (today.getDate() < 10) {\r\n        day = '0' + today.getDate();\r\n      } else {\r\n        day = today.getDate()\r\n      }\r\n\r\n      if (today.getMonth() + 1 < 10) {\r\n        month = '0' + (today.getMonth() + 1);\r\n      } else {\r\n        month = (today.getMonth() + 1);\r\n      }\r\n\r\n      year = today.getFullYear();\r\n\r\n      return `${day}.${month}.${year}`;\r\n    },\r\n    options() {\r\n      return this.$store.getters.getCategories\r\n    }\r\n  },\r\n  methods: {\r\n    ...mapActions([\r\n      'fetchCategoriesList'\r\n    ]),\r\n    onSave() {\r\n      const btn = document.querySelector('input[type=button]');\r\n      btn.disabled = true;\r\n      const {category, value} = this;\r\n      const data = {\r\n        date: this.date || this.getCurrentDate,\r\n        category,\r\n        value\r\n      };\r\n\r\n      if (data.category && data.value) {\r\n        btn.disabled = false;\r\n        this.$store.commit('addDataToPaymentsList', data);\r\n      }\r\n\r\n      \r\n      this.value = '';\r\n      this.date = '';\r\n    },\r\n  },\r\n  async created() {\r\n    await this.fetchCategoriesList();\r\n\r\n    if (this.$route?.params.page) {\r\n      this.category = this.$route.params.page;\r\n      this.value = this.$route.query.value;\r\n    }\r\n  },\r\n}\r\n</script>\r\n\r\n<style lang=\"sass\">\r\n\r\n  .modal\r\n    & input[type=button]\r\n      background-color: #20B799\r\n      font-size: 17px\r\n      font-weight: 700\r\n      text-transform: uppercase\r\n      border: 1px solid #20B799\r\n      border-radius: 3px\r\n      outline: none\r\n      padding: 10px 30px\r\n      color: white\r\n      cursor: pointer\r\n      &:hover\r\n        background-color: darken(#20B799, 10%) \r\n\r\n  .modal-form \r\n    width: 400px\r\n    & form\r\n      display: flex\r\n      flex-direction: column\r\n      & input, & select \r\n        padding: 10px 20px\r\n        margin-bottom: 20px\r\n        font-size: 17px\r\n        border: 1px solid lighten(black, 70%)\r\n        border-radius: 5px\r\n        outline: none\r\n\r\n</style>"],"sourceRoot":"src/components"}]}